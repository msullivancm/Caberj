/*/

User Function CRJP100X

	Local _aArea 	:= GetArea()
	Local _aAreBTS	:= BTS->(GetArea())
	Local _lRet		:= .T.
	Local _cEOL    	:= CHR(13)+CHR(10)
	Local _cAlias	:= GetNextAlias()
	Local _cNR		:= M->ZZ_NRCRNA
	Local _cMtVd	:= M->ZZ_CODVID

	BeginSql Alias _cAlias
		%noparser%

		SELECT
			BTS_MATVID,
			BTS_NOMUSR,
			BTS_CPFUSR,
			BTS_NRCRNA
		FROM
			%table:BTS% BTS
		WHERE
			BTS_FILIAL = %xFilial:BTS%
			AND BTS_NRCRNA = %Exp:_cNR%
			AND BTS_MATVID <> %Exp:_cMtVd%
			AND BTS_NRCRNA <> ' '
			AND BTS.%notDel%
	EndSql

	If !(_cAlias)->(Eof())

		Aviso("AtenГЦo","O Nr Cart Nac. informado jА encontra-se cadastrado no seguinte beneficiАrio: " + _cEOL;
			+ "Nome: " + AllTrim((_cAlias)->BTS_NOMUSR) + _cEOL + "Matric. Vida: " + (_cAlias)->BTS_MATVID, {"OK"})

		_lRet := .F.

	EndIf

	RestArea(_aAreBTS)
	RestArea(_aArea)

Return _lRet


