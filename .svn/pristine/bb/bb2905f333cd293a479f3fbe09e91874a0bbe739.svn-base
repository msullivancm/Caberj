// digito 2 da chave ASBACE
//User Function CALC_D2(cVariavel)
//Local Auxi := 0, sumdig := 0

cbase  := cVariavel+str(cDig1,1,0)
lbase  := LEN(cBase)
nBase  := 2
sumdig := 0
Auxi   := 0
iDig   := lBase
While iDig >= 1
	If nBase == 8
		nBase := 2
	EndIf
	auxi   := Val(SubStr(cBase, idig, 1)) * nBase
	sumdig := SumDig+auxi
	nBase := nBase + 1
	iDig:=iDig-1
EndDo
auxi := mod(sumdig,11)
If auxi == 1 .or. auxi == 10
	cDig1 := cDig1+1
	cbase  := cVariavel+str(cDig1,1,0)
	lbase  := LEN(cBase)
	nBase  := 2
	sumdig := 0
	Auxi   := 0
	iDig   := lBase
	While iDig >= 1
		If nBase == 8
			nBase := 2
		EndIf
		auxi   := Val(SubStr(cBase, idig, 1)) * nBase
		sumdig := SumDig+auxi
		nBase := nBase + 1
		iDig:=iDig-1
	EndDo
	auxi := mod(sumdig,11)
	If auxi == 1 .or. auxi == 10
		auxi := 0
	Else
		auxi := 11 - auxi
	EndIf
ElseIf auxi == 0
	auxi := 0
Else
	auxi := 11 - auxi
EndIf
cDig2 := auxi

Return(str(cDig1,1,0)+str(cDig2,1,0))
