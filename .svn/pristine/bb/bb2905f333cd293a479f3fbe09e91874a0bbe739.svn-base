User Function CALC_8p(cVariavel)

cbase := cVariavel
lbase  :=  LEN(cBase)
base := 2
sumdig := 0
Auxi   := 0
iDig   := lBase
While iDig >= 1
	If base = 10
		base = 2
	EndIf
	auxi   := Val(SubStr(cBase, idig, 1)) * base
	sumdig := SumDig+auxi
	base := base + 1
	iDig:=iDig-1
EndDo
auxi := mod(sumdig,11)
auxi := 11 - auxi
If auxi = 0 .or. auxi = 1 .or. auxi > 9
	auxi := 1
EndIf

Return(str(auxi,1,0))

