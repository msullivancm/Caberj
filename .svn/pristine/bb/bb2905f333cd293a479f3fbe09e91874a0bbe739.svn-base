User Function DIGIT453(cVariavel)
Local Auxi := 0, sumdig := 0

cbase  := cVariavel
lbase  := LEN(cBase)
base   := 2
sumdig := 0
Auxi   := 0
iDig   := lBase
While iDig >= 1
	auxi   := Val(SubStr(cBase, idig, 1)) * base
	If auxi > 10
		auxi := (auxi-10) + 1
	Endif
	sumdig := SumDig+auxi
	base   := 3 - base
	iDig   := iDig - 1
End
auxi := mod(sumdig,10)
auxi := 10 - auxi
If auxi >= 10 .or. auxi == 0
	auxi := "0"
Else
	auxi := str(Auxi,1,0)
Endif
Return( auxi )

