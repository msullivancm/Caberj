Static Function RunReport(Cabec1,Cabec2,Titulo,nLin)

Local nOrdem
Local cQuery := ' '
dbSelectArea(cString)
dbSetOrder(1)

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ SETREGUA -> Indica quantos registros serao processados para a regua ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

SetRegua(RecCount())    
 
 cQuery := " select ba1_codemp ||' - '|| substr(bg9_descri,1,20) EMP  , ba1_conemp conemp , ba1_subcon subcon , ba1_codven codven , ba1_codve2 codve2  ,count(*) QTDA ,  bqc_descri DESCRISUB "
 cQuery += "  from " + RetSqlName("BA1")+" BA1, " + RetSqlName("BG9")+" BG9 , " + RetSqlName("BQC")+" BQC "
 cQuery += " where ba1_filial = '"+xFilial("BA1")+"'  and bA1.d_E_L_E_T_ = ' ' "
 cQuery += "   and bg9_filial = '"+xFilial("BG9")+"'  and bg9.d_E_L_E_T_ = ' '	"      
 cQuery += "   and bg9_filial = '"+xFilial("BQC")+"'  and BQC.d_E_L_E_T_ = ' '	" 
 cQuery += "   and ba1_codemp = '"+cEmp+"'"
 cQuery += "   and bg9_codigo = ba1_codemp "           
 cQuery += "   and bqc_codigo = '0001'||ba1_codemp and  ba1_conemp = bqc_numcon and  ba1_subcon = bqc_subcon "
 cQuery += " group by ba1_codemp ||' - '|| substr(bg9_descri,1,20) , ba1_conemp , ba1_subcon , ba1_codven , ba1_codve2 , bqc_descri"
 cQuery += " order by ba1_codemp ||' - '|| substr(bg9_descri,1,20) , ba1_subcon , ba1_codven , ba1_codve2 "


 If Select(("TMP")) <> 0 
       ("TMP")->(DbCloseArea())  
    Endif
    TCQuery cQuery Alias "TMP" New 
    dbSelectArea("TMP")
    tmp->(dbGoTop())       
    
    While !EOF()                                                                
	  If nLin > 65 // Salto de Pแgina. Neste caso o formulario tem 55 linhas...
	   nLin :=	Cabec(Titulo,Cabec1,Cabec2,NomeProg,Tamanho,nTipo)
	   nLin ++
	Endif
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Verifica o cancelamento pelo usuario...                             ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	If lAbortPrint
		@nLin,00 PSAY "*** CANCELADO PELO OPERADOR ***"
		Exit
	Endif                                                     

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Impressao do cabecalho do relatorio. . .                            ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู  

// cQuery := " select ba1_codemp ||' - '|| substr(bg9_descri,1,20) EMP  , ba1_conemp conemp , ba1_subcon subcon , ba1_codven codven , ba1_codve2 codve2  ,count(*) QTDA "

	 @ nLin,001 PSay  tmp->EMP
	 @ nLin,040 PSay  tmp->conemp
	 @ nLin,060 PSay  tmp->subcon
	 @ nLin,075 PSay  tmp->codven  
	 @ nLin,085 PSay  tmp->codve2            
	 @ nLin,099 PSay  tmp->qtda             
	 @ nLin,110 PSay  tmp->DESCRISUB            
	 
	nLin ++     // Avanca a linha de impressao
	dbSkip()   // Avanca o ponteiro do registro no arquivo
EndDo    
    @ nLin,000 PSay replicate("_",220)  


//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Finaliza a execucao do relatorio...                                 ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

SET DEVICE TO SCREEN

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Se impressao em disco, chama o gerenciador de impressao...          ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

If aReturn[5]==1
	dbCommitAll()
	SET PRINTER TO
	OurSpool(wnrel)
Endif

MS_FLUSH()

Return


