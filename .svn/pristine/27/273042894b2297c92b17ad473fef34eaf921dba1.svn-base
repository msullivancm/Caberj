Static Function RunReport(Cabec1,Cabec2,Titulo,nLin)

Local nOrdem
Local cQuery := ' '
dbSelectArea(cString)
dbSetOrder(1)

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ SETREGUA -> Indica quantos registros serao processados para a regua ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

SetRegua(RecCount())       
cQuery := " select  e1_codint||'.'||e1_codemp||'.'||e1_matric ||' - '|| e1_nomcli matric , "
cQuery += " E1_prefixo , e1_NUM , E1_tipo , e1_valor , e1_saldo , e1_baixa , BA3_GRPCOB,  ba3_tippag "
cQuery += " from se1010  se1, ba3010 ba3 where e1_filial = '01' and se1.d_e_l_e_t_ = ' ' "
cQuery += " and ba3_filial = ' ' and ba3.d_e_l_e_t_ = ' ' and e1_origem = 'PLSA510' "
cQuery += " AND E1_BAIXA >='" +cDatinc+"' AND E1_BAIXA <='"+cDatFim+"' "
cQuery += " and e1_mesbase = '"+cmesbase+"' and e1_anobase = '"+canobase+"' "      
if !empty(cFormCob)
    cQuery += " and ba3_tippag = '"+cFormCob+"'"
EndIf    
cQuery += " and e1_codint = ba3_codint and e1_codemp = ba3_codemp and  e1_matric = ba3_matric "
cQuery += " order by BA3_GRPCOB,  ba3_tippag, E1_prefixo , e1_NUM , E1_tipo , e1_valor , e1_saldo , e1_baixa "
   

 If Select(("TMP")) <> 0 
       ("TMP")->(DbCloseArea())  
    Endif
    TCQuery cQuery Alias "TMP" New 
    dbSelectArea("TMP")
    tmp->(dbGoTop())       
    
    While !EOF()                                                                
	  If nLin > 65 // Salto de Pแgina. Neste caso o formulario tem 55 linhas...
	   nLin :=	Cabec(Titulo,Cabec1,Cabec2,NomeProg,Tamanho,nTipo)
	   nLin ++
	Endif
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Verifica o cancelamento pelo usuario...                             ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	If lAbortPrint
		@nLin,00 PSAY "*** CANCELADO PELO OPERADOR ***"
		Exit
	Endif                                                     

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Impressao do cabecalho do relatorio. . .                            ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู  
	 @ nLin,000 PSay  tmp->matric
	 @ nLin,065 PSay  tmp->E1_prefixo
	 @ nLin,070 PSay  tmp->e1_NUM
	 @ nLin,080 PSay  tmp->E1_tipo  
	 @ nLin,086 PSAY  tmp->e1_valor Picture "@E 999,999,999.99"  
	 @ nLin,100 PSAY  tmp->e1_saldo Picture "@E 999,999,999.99"                                                               
         
	 @ nLin,117 PSay  stod(tmp->e1_baixa)            
	 @ nLin,130 PSay  tmp->BA3_GRPCOB            
	 @ nLin,140 PSay  tmp->ba3_tippag                                                             
	 vlr_tot      += tmp->e1_valor   
     vlr_Sald     += tmp->e1_saldo                                                                      
                                        

	nLin ++     // Avanca a linha de impressao
	dbSkip()   // Avanca o ponteiro do registro no arquivo
EndDo    
    @ nLin,000 PSay replicate("_",220)  
    nLin ++                              
    @ nLin,086 PSAY  vlr_tot  Picture "@E 999,999,999.99"  
    @ nLin,100 PSAY  vlr_Sald Picture "@E 999,999,999.99"         
    nLin ++
    @ nLin,000 PSay replicate("_",220)                            
  
    
    


//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Finaliza a execucao do relatorio...                                 ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

SET DEVICE TO SCREEN

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Se impressao em disco, chama o gerenciador de impressao...          ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

If aReturn[5]==1
	dbCommitAll()
	SET PRINTER TO
	OurSpool(wnrel)
Endif

MS_FLUSH()

Return


