User Function CABA588A(_cParam)
	
	Local _aArea 	:= GetArea()
	Local _cRet	 	:= ""
	
	Default _cParam := "1"
	
	If _cParam == "1"
		
		PswOrder(1)
		If PswSeek(M->PCY_USUARIO, .T.)
			
			_cRet := PswRet(1)[1][4]
			
		Else
			
			Aviso("Atenção","Usuário não localizado no sistema ou campo não preenchido no protocolo.",{"OK"})
			
		EndIf
		
	Else
		
		PswOrder(1)
		If PswSeek(PCY->PCY_USUARIO, .T.)
			
			_cRet := PswRet(1)[1][4]
			
		EndIf
		
	EndIf
	
	RestArea(_aArea)
	
Return _cRet