Static Function CTR110MaxL(nMaxLin,nLinReport, cPicture )
Local oSection1 := oReport:Section(1)
Local oSection3 := oReport:Section(3)
Local nMaxLin1       

nMaxLin1 := nMaxLin   

If oSection1:Printing()
	nLinReport += 2
	If MV_PAR15 == 1 
		nLinReport++
	EndIf
Else
	nLinReport++
Endif

If nLinReport > nMaxLin1 - 2
	If nTransC > 0 .OR. nTransD > 0
	  
		oSection3:Init()
		oSection3:Printline() 
		oSection3:Finish()
	                      
 		oReport:EndPage()
		nLinReport := 11 	
		oSection3:Init()
		oSection3:Printline() 
		oSection3:Finish()
		oReport:Skipline() 
	
    Else 
    
  		nLinReport := 9  
	                      
 		oReport:EndPage()
 	
	EndIf 	
Else       
EndIf   


Return Nil

